@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ex:DMNObjectShape a sh:NodeShape ;
    sh:targetClass dmn:Objects ;
    sh:property [
        sh:path dmn:hasActivity ;
        sh:minCount 1 ;
        sh:message "Each dmn:Object must have at least one dmn:hasActivity property." ;
    ] ;
    sh:property [
        sh:path dicm:hasMaterial ;
        sh:minCount 1 ;
        sh:message "Each dmn:Object must have at least one dicm:hasMaterial property." ;
    ] .
ex:DMNActivityShape a sh:NodeShape ;
    sh:targetClass dicp:Activity ;
    sh:property [
        sh:path dmn:hasStartDate ;
        sh:minCount 1 ;
        sh:message "Each dicp:Activity must have at least one dmn:hasStartDate property." ;
    ] .