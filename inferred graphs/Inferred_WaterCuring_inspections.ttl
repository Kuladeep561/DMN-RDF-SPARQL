@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix dica: <https://w3id.org/digitalconstruction/0.5/Agents#> .
@prefix dice: <https://w3id.org/digitalconstruction/0.5/Entities#> .
@prefix dicm: <https://w3id.org/digitalconstruction/0.5/Materials#> .
@prefix dicp: <https://w3id.org/digitalconstruction/0.5/Processes#> .
@prefix dmn: <http://www.DMN-RDF.org/DMN#> .
@prefix ex: <http://example.org/#> .
@prefix ifc: <http://ifc.org/object/#> .
@prefix ocqa: <https://w3id.org/ocqa#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ex:hasActivity a owl:ObjectProperty .

ifc:ifc_slab_1234 a ex:Objects ;
    ex:hasActivity ex:Water_Curing ;
    dicm:hasMaterial ex:Concrete ;
    ocqa:hasInspection dmn:Inspection_minimum-curing-period,
        dmn:Inspection_pH-value-of-water,
        dmn:Inspection_water-quality-for-foreign-material .

ifc:ifc_slab_1235 a ex:Objects ;
    ex:hasActivity ex:Water_Curing ;
    dicm:hasMaterial ex:Concrete ;
    ocqa:hasInspection dmn:Inspection_minimum-curing-period,
        dmn:Inspection_pH-value-of-water,
        dmn:Inspection_water-quality-for-foreign-material .

ifc:ifc_slab_1236 a ex:Objects ;
    ex:hasActivity ex:Water_Curing ;
    dicm:hasMaterial ex:Concrete ;
    ocqa:hasInspection dmn:Inspection_minimum-curing-period,
        dmn:Inspection_pH-value-of-water,
        dmn:Inspection_water-quality-for-foreign-material .

ifc:ifc_slab_1237 a ex:Objects ;
    ex:hasActivity ex:Water_Curing ;
    dicm:hasMaterial ex:Concrete ;
    ocqa:hasInspection dmn:Inspection_minimum-curing-period,
        dmn:Inspection_pH-value-of-water,
        dmn:Inspection_water-quality-for-foreign-material .

ifc:ifc_slab_1238 a ex:Objects ;
    ex:hasActivity ex:Water_Curing .

dcterms:description a owl:AnnotationProperty .

dcterms:title a owl:AnnotationProperty .

vann:preferredNamespacePrefix a owl:AnnotationProperty .

vann:preferredNamespaceUri a owl:AnnotationProperty .

dmn: a owl:Ontology ;
    dcterms:description "DMN to RDF." ;
    dcterms:title "DMN to RDF" ;
    vann:preferredNamespacePrefix "dmn" ;
    vann:preferredNamespaceUri "http://www.DMN-RDF.org/DMN#" ;
    owl:versionIRI <http://www.DMN-RDF.org/DMN/1.0> ;
    owl:versionInfo 1.0 .

dmn:Concrete a dicm:Material ;
    dmn:hasActivity dmn:Curing .

dmn:hasActivity a owl:ObjectProperty ;
    rdfs:range dicp:Activity .

dmn:hasCodeOfConduct a owl:ObjectProperty ;
    rdfs:range dmn:Norms .

dmn:hasEndDate a owl:DatatypeProperty .

dmn:hasFrequency a owl:DatatypeProperty .

dmn:hasStartDate a owl:DatatypeProperty .

owl:minQualifiedCardinality a owl:AnnotationProperty .

dica:hasApprover a owl:ObjectProperty ;
    rdfs:label "hasApprover" ;
    rdfs:subPropertyOf dica:hasAgent .

dica:hasResponsibleAgent a owl:ObjectProperty ;
    rdfs:label "hasResponsibleAgent" ;
    rdfs:comment "The agent of activity with overall reponsibility that the activity gets executed" ;
    rdfs:subPropertyOf dica:hasAgent .

dice:hasPart a owl:ObjectProperty ;
    owl:inverseOf dice:isPartOf .

dicm:CompositeMaterial a owl:Class ;
    rdfs:label "CompositeMaterial" ;
    rdfs:subClassOf dicm:Material .

dicm:PHLevel a owl:Class ;
    rdfs:label "PHLevel" ;
    rdfs:comment "Maximum water PH in a range from 0-14."^^xsd:string ;
    rdfs:subClassOf dicm:MaterialChemicalProperty .

dicm:hasPHLevel a owl:DatatypeProperty ;
    rdfs:label "hasPHLevel" ;
    rdfs:comment "Maximum water PH in a range from 6-8."^^xsd:string ;
    rdfs:domain dicm:Material .

dicp:hasLocation a owl:ObjectProperty ;
    rdfs:domain dicp:Activity ;
    rdfs:range dice:Location .

dicp:hasObject a owl:ObjectProperty ;
    rdfs:label "hasObject" ;
    rdfs:comment "The object that an activity acts on or is focused to. For example, a transportation activity can have as its object a container, or painting can have as its object a wall." ;
    rdfs:domain dicp:Activity ;
    rdfs:range dice:Entity ;
    owl:inverseOf dicp:isObjectIn .

dicp:hasSubActivity a owl:ObjectProperty ;
    rdfs:label "hasSubActivity" ;
    rdfs:domain dicp:Activity ;
    rdfs:range dicp:Activity ;
    rdfs:subPropertyOf dice:hasProperOccurrentPart ;
    owl:inverseOf dicp:isSubActivityOf .

ocqa:hasInspection a owl:ObjectProperty ;
    rdfs:label "hatPrüfung"@de ;
    rdfs:comment "Eine Beziehung zwischen einem Prüfplan und einer Inspection"@de ;
    rdfs:domain ocqa:InspectionPlan ;
    rdfs:range ocqa:Inspection .

ocqa:hasInspectionEquipment a owl:ObjectProperty ;
    rdfs:domain ocqa:Inspection ;
    rdfs:range ocqa:InspectionEquipment .

ocqa:hasInspectionProcedure a owl:ObjectProperty ;
    rdfs:domain ocqa:Inspection ;
    rdfs:range ocqa:InspectionProcedure ;
    rdfs:subPropertyOf ocqa:hasProcedure .

dmn:Confirm-minimum-days-for-water-curing-according-to_IS-456 a ocqa:InspectionProcedure .

dmn:Curing a dicp:Activity ;
    dicp:hasSubActivity dmn:WaterCuring_1 .

dmn:Drawings-Designs a ocqa:InspectionEquipment .

dmn:ISCode a owl:Class ;
    dcterms:description "Indian standard code ."^^xsd:string ;
    rdfs:subClassOf dmn:Norms .

dmn:Litmuspaper a ocqa:InspectionEquipment .

dmn:SubActivity a owl:Class ;
    rdfs:label "Object",
        "SubActivity" ;
    rdfs:comment "",
        "A process that is intentionally performed by an agent." ;
    rdfs:subClassOf dicp:Activity .

dmn:Testing-procedure-of-finding-impurities-in-water-according-to_IS-456 a ocqa:InspectionProcedure .

dmn:Testing-procedure-of-finding-pH-value-according-to_IS-456 a ocqa:InspectionProcedure .

dmn:WaterCuring_1 a dmn:SubActivity ;
    dmn:hasStartDate "2023-03-17T00:00:00+00:00"^^xsd:dateTime ;
    ocqa:hasInspection dmn:Inspection_minimum-curing-period,
        dmn:Inspection_pH-value-of-water,
        dmn:Inspection_water-quality-for-foreign-material .

dmn:visual-inspection a ocqa:InspectionEquipment .

dica:isAgentIn a owl:ObjectProperty .

dice:MaterialEntity a owl:Class .

dice:Site a owl:Class .

dice:hasParticipant a owl:ObjectProperty .

dice:hasProperOccurrentPart a owl:ObjectProperty .

dice:isPartOf a owl:ObjectProperty ;
    rdfs:label "isPartOf" ;
    rdfs:domain dice:Entity ;
    rdfs:range dice:Entity ;
    rdfs:subPropertyOf owl:topObjectProperty .

dicm:MaterialChemicalProperty a owl:Class .

dicm:hasConstituent a owl:ObjectProperty .

dicm:hasLayer a owl:ObjectProperty .

dicm:hasProfile a owl:ObjectProperty .

dicp:isObjectIn a owl:ObjectProperty .

dicp:isSubActivityOf a owl:ObjectProperty .

ocqa:Determination a owl:Class .

ocqa:InspectionPlan a owl:Class .

ocqa:Procedure a owl:Class .

ocqa:hasProcedure a owl:ObjectProperty ;
    rdfs:domain dicp:Activity ;
    rdfs:range ocqa:Procedure .

dmn:Norms a owl:Class ;
    dcterms:description "A building code is a set of rules that specify the standards for constructed objects such as buildings and non-building structures ."^^xsd:string .

dica:Agent a owl:Class .

dica:hasAgent a owl:ObjectProperty ;
    rdfs:label "hasAgent" ;
    rdfs:domain dice:Process ;
    rdfs:range dica:Agent ;
    rdfs:subPropertyOf dice:hasParticipant ;
    owl:inverseOf dica:isAgentIn .

dice:Location a owl:Class ;
    rdfs:label "Location" ;
    rdfs:comment "A (logical) place where material entities can be located or activities can occur. Locations can form a hierarchy, for example, in a location breakdown structure consisting of a site, buildings, floors, zones, spaces, and workspaces" ;
    rdfs:subClassOf dice:Site .

dice:Process a owl:Class .

dmn:At-workingSite a dice:Location .

dmn:IS456-2000 a dmn:ISCode .

dmn:SiteEngineer a dica:Agent .

dice:Entity a owl:Class .

dicm:hasMaterial a owl:ObjectProperty ;
    rdfs:label "hasMaterial" ;
    rdfs:range dicm:Material ;
    owl:propertyChainAxiom ( dicm:hasConstituent dicm:hasMaterial ),
        ( dicm:hasLayer dicm:hasMaterial ),
        ( dicm:hasProfile dicm:hasMaterial ) .

dicm:Material a owl:Class ;
    rdfs:label "Material" ;
    dcterms:description "a homogeneous or inhomogeneous substance that can be used to form elements (physical products or their components). [ISO 16739]"^^xsd:string ;
    rdfs:subClassOf dice:MaterialEntity .

ocqa:InspectionEquipment a owl:Class .

ocqa:InspectionProcedure a owl:Class .

dmn:Inspection_minimum-curing-period a ocqa:Inspection ;
    dmn:hasEndDate "2023-03-18T00:00:00+00:00"^^xsd:dateTime ;
    dmn:hasFrequency dmn:oneTime ;
    rdfs:comment "Minimum curing period is 7 days"^^xsd:string ;
    dica:hasAgent dmn:SiteEngineer ;
    dice:isPartOf dmn:IS456-2000 ;
    dicp:hasLocation dmn:At-workingSite ;
    ocqa:hasInspectionEquipment dmn:Drawings-Designs ;
    ocqa:hasInspectionProcedure dmn:Confirm-minimum-days-for-water-curing-according-to_IS-456 .

dmn:Inspection_pH-value-of-water a ocqa:Inspection ;
    dmn:hasEndDate "2023-03-18T00:00:00+00:00"^^xsd:dateTime ;
    dmn:hasFrequency dmn:oneTime ;
    rdfs:comment "Permissible pH value is between 6 to 8"^^xsd:string ;
    dica:hasAgent dmn:SiteEngineer ;
    dice:isPartOf dmn:IS456-2000 ;
    dicp:hasLocation dmn:At-workingSite ;
    ocqa:hasInspectionEquipment dmn:Litmuspaper ;
    ocqa:hasInspectionProcedure dmn:Testing-procedure-of-finding-pH-value-according-to_IS-456 .

dmn:Inspection_water-quality-for-foreign-material a ocqa:Inspection ;
    dmn:hasEndDate "2023-03-18T00:00:00+00:00"^^xsd:dateTime ;
    dmn:hasFrequency dmn:oneTime ;
    rdfs:comment "Should be free from foreign material"^^xsd:string ;
    dica:hasAgent dmn:SiteEngineer ;
    dice:isPartOf dmn:IS456-2000 ;
    dicp:hasLocation dmn:At-workingSite ;
    ocqa:hasInspectionEquipment dmn:visual-inspection ;
    ocqa:hasInspectionProcedure dmn:Testing-procedure-of-finding-impurities-in-water-according-to_IS-456 .

ocqa:Inspection a owl:Class ;
    rdfs:subClassOf ocqa:Determination .

dicp:Activity a owl:Class ;
    rdfs:label "Activity" ;
    rdfs:comment "A process that is intentionally performed by an agent." ;
    rdfs:subClassOf dice:Process .

[] a sh:ValidationReport ;
    sh:conforms true .

