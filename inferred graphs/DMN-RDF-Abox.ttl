@prefix dica: <https://w3id.org/digitalconstruction/0.5/Agents#> .
@prefix dice: <https://w3id.org/digitalconstruction/0.5/Entities#> .
@prefix dicm: <https://w3id.org/digitalconstruction/0.5/Materials#> .
@prefix dicp: <https://w3id.org/digitalconstruction/0.5/Processes#> .
@prefix dmn: <http://www.DMN-RDF.org/DMN#> .
@prefix ocqa: <https://w3id.org/ocqa#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

dmn:Concrete a dicm:Material ;
    dmn:hasActivity dmn:Curing .

dmn:Confirm-minimum-days-for-water-curing-according-to_IS-456 a ocqa:InspectionProcedure .

dmn:Curing a dicp:Activity ;
    dicp:hasSubActivity dmn:WaterCuring_1 .

dmn:Drawings-Designs a ocqa:InspectionEquipment .

dmn:Inspection_minimum-curing-period a ocqa:Inspection ;
    dmn:hasEndDate "2023-03-18T00:00:00+00:00"^^xsd:dateTime ;
    dmn:hasFrequency dmn:oneTime ;
    rdfs:comment "Minimum curing period is 7 days"^^xsd:string ;
    dica:hasAgent dmn:SiteEngineer ;
    dice:isPartOf dmn:IS456-2000 ;
    dicp:hasLocation dmn:At-workingSite ;
    ocqa:hasInspectionEquipment dmn:Drawings-Designs ;
    ocqa:hasInspectionProcedure dmn:Confirm-minimum-days-for-water-curing-according-to_IS-456 .

dmn:Inspection_pH-value-of-water a ocqa:Inspection ;
    dmn:hasEndDate "2023-03-18T00:00:00+00:00"^^xsd:dateTime ;
    dmn:hasFrequency dmn:oneTime ;
    rdfs:comment "Permissible pH value is between 6 to 8"^^xsd:string ;
    dica:hasAgent dmn:SiteEngineer ;
    dice:isPartOf dmn:IS456-2000 ;
    dicp:hasLocation dmn:At-workingSite ;
    ocqa:hasInspectionEquipment dmn:Litmuspaper ;
    ocqa:hasInspectionProcedure dmn:Testing-procedure-of-finding-pH-value-according-to_IS-456 .

dmn:Inspection_water-quality-for-foreign-material a ocqa:Inspection ;
    dmn:hasEndDate "2023-03-18T00:00:00+00:00"^^xsd:dateTime ;
    dmn:hasFrequency dmn:oneTime ;
    rdfs:comment "Should be free from foreign material"^^xsd:string ;
    dica:hasAgent dmn:SiteEngineer ;
    dice:isPartOf dmn:IS456-2000 ;
    dicp:hasLocation dmn:At-workingSite ;
    ocqa:hasInspectionEquipment dmn:visual-inspection ;
    ocqa:hasInspectionProcedure dmn:Testing-procedure-of-finding-impurities-in-water-according-to_IS-456 .

dmn:Litmuspaper a ocqa:InspectionEquipment .

dmn:Testing-procedure-of-finding-impurities-in-water-according-to_IS-456 a ocqa:InspectionProcedure .

dmn:Testing-procedure-of-finding-pH-value-according-to_IS-456 a ocqa:InspectionProcedure .

dmn:WaterCuring_1 a dmn:SubActivity ;
    dmn:hasStartDate "2023-03-17T00:00:00+00:00"^^xsd:dateTime ;
    ocqa:hasInspection dmn:Inspection_minimum-curing-period,
        dmn:Inspection_pH-value-of-water,
        dmn:Inspection_water-quality-for-foreign-material .

dmn:visual-inspection a ocqa:InspectionEquipment .

dmn:At-workingSite a dice:Location .

dmn:IS456-2000 a dmn:ISCode .

dmn:SiteEngineer a dica:Agent .

